{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc",
    "typecheck:watch": "tsc -w",
    "lint": "eslint src --max-warnings=0",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "jest --runInBand",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s ./public"
  },
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "@headlessui/react": "^1.4.0",
    "@hookform/resolvers": "^2.8.1",
    "axios": "^0.21.4",
    "date-fns": "^2.25.0",
    "graphql": "^15.5.1",
    "next": "^11.0.1",
    "next-auth": "^3.29.0",
    "next-pwa": "^5.2.23",
    "nextjs-progressbar": "^0.0.11",
    "plop": "^2.7.4",
    "react": "17.0.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.15.3",
    "react-slick": "^0.28.1",
    "react-use": "^17.3.1",
    "styled-components": "^5.2.3",
    "styled-media-query": "^2.1.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/addon-essentials": "^6.5.7",
    "@storybook/react": "^6.3.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/react-custom-scrollbars": "^4.0.8",
    "@types/react-dom": "^17.0.9",
    "@types/react-slick": "^0.23.5",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.20.0",
    "apollo": "^2.33.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-styled-components": "^7.0.4",
    "lint-staged": "^11.1.2",
    "prettier": "2.4.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "4.3.5"
  }
}
